#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/c/Users/USER/Downloads/Desktop/API/node_modules/.pnpm/json-server@0.17.4/node_modules/json-server/lib/cli/node_modules:/proc/cygdrive/c/Users/USER/Downloads/Desktop/API/node_modules/.pnpm/json-server@0.17.4/node_modules/json-server/lib/node_modules:/proc/cygdrive/c/Users/USER/Downloads/Desktop/API/node_modules/.pnpm/json-server@0.17.4/node_modules/json-server/node_modules:/proc/cygdrive/c/Users/USER/Downloads/Desktop/API/node_modules/.pnpm/json-server@0.17.4/node_modules:/proc/cygdrive/c/Users/USER/Downloads/Desktop/API/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/c/Users/USER/Downloads/Desktop/API/node_modules/.pnpm/json-server@0.17.4/node_modules/json-server/lib/cli/node_modules:/proc/cygdrive/c/Users/USER/Downloads/Desktop/API/node_modules/.pnpm/json-server@0.17.4/node_modules/json-server/lib/node_modules:/proc/cygdrive/c/Users/USER/Downloads/Desktop/API/node_modules/.pnpm/json-server@0.17.4/node_modules/json-server/node_modules:/proc/cygdrive/c/Users/USER/Downloads/Desktop/API/node_modules/.pnpm/json-server@0.17.4/node_modules:/proc/cygdrive/c/Users/USER/Downloads/Desktop/API/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/cli/bin.js" "$@"
else
  exec node  "$basedir/../../lib/cli/bin.js" "$@"
fi
